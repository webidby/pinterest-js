idth: 100%;\r\n        padding: 12px;\r\n    }\r\n}\r\n\r\n.pins-container {\r\n    display: grid;\r\n    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\r\n    gap: 16px;\r\n    padding: 20px;\r\n    margin-top: 20px;\r\n}\r\n\r\n\r\n.pin {\r\n    position: relative;\r\n    background-color: #f7f7f7;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n    cursor: pointer;\r\n    transition: transform 0.3s ease;\r\n    width: 250px;\r\n    height: 350px;\r\n}\r\n\r\n.pin:hover {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.pin img {\r\n    width: 100%;\r\n    height: 90%;\r\n    display: block;\r\n    object-fit: cover;\r\n}\r\n\r\n\r\n.pin-description {\r\n    padding: 10px;\r\n    font-size: 14px;\r\n    color: #333;\r\n    background-color: #f7f7f7;\r\n    text-align: center;\r\n    position: relative;\r\n}\r\n\r\n\r\n.circle-btn {\r\n    position: absolute;\r\n    bottom: 70px;\r\n    left: 90%;\r\n    transform: translateX(-50%);\r\n    width: 30px;\r\n    height: 30px;\r\n    background-color: #f46262c7;\r\n    border: none;\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n    opacity: 0;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    transition: background-color 0.3s ease;\r\n}\r\n\r\n.circle-btn:hover {\r\n    background-color: #d42424;\r\n}\r\n\r\n.pin:hover .circle-btn {\r\n    opacity: 1;\r\n}\r\n\r\n\r\n@media (max-width: 768px) {\r\n    .pins-container {\r\n        grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\r\n    }\r\n}\r\n\r\n/* Стили для board-selector */\r\n.board-selector {\r\n    width: 100%;\r\n    max-width: 400px;\r\n    margin: 20px auto;\r\n    padding: 20px;\r\n    background-color: #f8f8f8;\r\n    text-align: center;\r\n}\r\n\r\n.board-selector select {\r\n    width: 60%;\r\n    padding: 10px;\r\n    margin: 10px 0;\r\n    font-size: 16px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n}\r\n\r\n.board-selector button {\r\n    padding: 10px 20px;\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n}\r\n\r\n.board-selector button:hover {\r\n    background-color: #45a049;\r\n}\r\n\r\n.board-selector .board-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-top: 20px;\r\n}"},"lineCount":194}},"error":null,"hash":"74f71a3820f8f322291f99c374a36b45","cacheData":{"env":{}}}